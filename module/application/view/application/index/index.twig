<div class="jumbotron">
    <h1 class="display-4">Welcome to<br>Eureka Clusters Backend</h1>
</div>


<p class="lead">{{ translate("txt-choose-one-of-the-services") }}</p>

{% for service in services %}
    <a class="btn btn-primary btn-lg"
       href="{{ url('oauth2/login',{'id':service.id,'name':service.name}) }}">{{ translate("txt-login-via-%s")|format(service.name) }}</a>

{% endfor %}